  "description": "This repository implements a **Mock Solana DEX Order Execution Engine** that simulates routing, quoting, execution latency, retries, and real-time status updates.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec ts-node src/api/server.ts",
    "worker": "ts-node src/worker/index.ts",
    "db:setup": "ts-node src/db/setup.ts",
    "start": "ts-node src/api/server.ts",
    "build": "tsc",
    "start:api": "node dist/api/server.js",
    "start:worker": "node dist/worker/index.js",
    "start:all": "concurrently -k \"node dist/api/server.js\" \"node dist/worker/index.js\"",
    "demo": "ts-node src/demo.ts"
  },
